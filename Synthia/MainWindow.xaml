                    </WrapPanel>
                </GroupBox>

            </WrapPanel>

            <local:WaveFormElement x:Name="waveFormElement"
                Height="100"
                Width="1300" Margin="0,5,0,0"
                />
        </WrapPanel>
    </Grid>
</Window>
